  border-radius: 50%;
  transform: translate(-50%, -50%);
  animation: pulse 1.5s infinite;
}

.data-absorption {
  position: absolute;
  left: 20%;
  top: 20%;
  width: 20px;
  height: 60px;
}

.data-point {
  position: absolute;
  width: 6px;
  height: 6px;
